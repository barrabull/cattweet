@article{Fama1965,
  added-at = {2013-05-01T08:15:43.000+0200},
  author = {Fama, E. F.},
  biburl = {http://www.bibsonomy.org/bibtex/262ba528f6694457393fd924145847858/krassi},
  comment = {Fama doctoral dissertation, reproduced here in its
                 entirety. Fama was Mandelbrot's student. Empirical
                 tests of the random walk nature of stock price behavior
                 Confirms that daily stock log-prices of large mature US
                 companies follow Mandelbrot stable Paretian random walk
                 - independence * daily returns are independent (no
                 serial correlation, runs analysis, Alexander filter) *
                 this is consistent with an efficient market -
                 non-normality * S-shaped normality plots for daily
                 returns * this leptokurtosis cannot be explained by a
                 mixture of Gaussians (e.g. weekends) or
                 non-stationarity (varying trends) - stability * similar
                 S-shaped for returns over 4 days * the distribution
                 would not change over addition - stable Pareto
                 distribution with alpha < 2 * inspired by the
                 Mandelbrot hypothesis, originally tested on cotton
                 prices in 1963 * consequence: prices tend to jump *
                 this must reflect jumps in the intrinsic value,
                 consistent with a dynamic economy in a world of
                 uncertainty - mutual funds fail to outperform
                 consistently the market (empirical survey) * we should
                 use the mean absolute deviation instead of standard
                 deviation to measure dispersion, because it is more
                 stable (with alpha<2, the sample standard deviation has
                 an erratic behaviour when the sample size increases,
                 because the population stadard deviation is not
                 defined) Introduces the concepts of intrinsic value,
                 sophisticated trader},
  interhash = {86ea5ab29796daba41f2af8937acdad6},
  intrahash = {62ba528f6694457393fd924145847858},
  journal = {Journal of Business},
  keywords = {behaviour markets stock},
  number = 1,
  pages = {34--105},
  timestamp = {2013-05-01T08:15:43.000+0200},
  title = {The behavior of stock-market prices},
  volume = 38,
  year = 1965
}

